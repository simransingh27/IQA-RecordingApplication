@model IEnumerable<IQA_RecordingApplication.Models.SKUCodeViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               <h2>SKU Code: </h2>
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SKUCodeDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedAt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SKUCodeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SKUCodeDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedAt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SKUCodeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SKUCodeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SKUCodeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
